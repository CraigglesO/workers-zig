{
  "name": "workers-zig",
  "type": "module",
  "description": "Write Cloudflare Workers in Zig via WebAssembly",
  "private": false,
  "version": "0.1.6",
  "keywords": [
    "cloudflare",
    "worker",
    "workers",
    "serverless",
    "zig",
    "workers zig",
    "cf",
    "parameters",
    "typescript",
    "esm",
    "typed"
  ],
  "types": "./distESM/index.d.ts",
  "typescript": {
    "main": "src/index.ts"
  },
  "directories": {
    "lib": "distESM"
  },
  "main": "./distESM/index.js",
  "exports": {
    ".": "./distESM/index.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "distESM/index.d.ts"
      ]
    }
  },
  "author": "Craig O'Connor <craiggles@s2maps.io>",
  "license": "MIT",
  "homepage": "https://github.com/CraigglesO/workers-zig",
  "repository": {
    "type": "git",
    "url": "git@github.com:CraigglesO/workers-zig.git"
  },
  "bugs": {
    "url": "https://github.com/CraigglesO/workers-zig/issues"
  },
  "scripts": {
    "dev": "wrangler dev",
    "local": "wrangler dev --local --experimental-enable-local-persistence --ip 127.0.0.1 --port 8789",
    "build:esbuild": "rm -rf dist && mkdir dist && node esbuild.js && node esbuildWasi.js",
    "build:zig": "zig build",
    "wrangler:publish": "wrangler publish",
    "test": "npm run build:zig && npm run build:esbuild && NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" vitest run"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "d1testflare": "^2.7.5",
    "esbuild": "^0.14.53",
    "esbuild-plugin-wat": "^0.2.7",
    "itty-router": "^2.6.1",
    "miniflare": "^2.7.1",
    "typescript": "^4.7.4",
    "uglify-js": "^3.17.0",
    "vitest": "^0.23.1",
    "wrangler": "2.0.26"
  },
  "dependencies": {
    "@cloudflare/workers-wasi": "^0.0.5"
  }
}
