{
  "name": "workers-zig",
  "type": "module",
  "main": "dist/worker.mjs",
  "module": "dist/worker.mjs",
  "description": "Write Cloudflare Workers in Zig via WebAssembly",
  "private": false,
  "version": "0.1.3",
  "keywords": [
    "cloudflare",
    "worker",
    "workers",
    "serverless",
    "zig",
    "workers zig",
    "cf",
    "parameters",
    "typescript",
    "esm",
    "typed"
  ],
  "author": "Craig O'Connor <craiggles@s2maps.io>",
  "license": "MIT",
  "homepage": "https://github.com/CraigglesO/workers-zig",
  "repository": {
    "type": "git",
    "url": "git@github.com:CraigglesO/workers-zig.git"
  },
  "bugs": {
    "url": "https://github.com/CraigglesO/workers-zig/issues"
  },
  "scripts": {
    "dev": "wrangler dev",
    "local": "wrangler dev --local --experimental-enable-local-persistence --ip 127.0.0.1 --port 8789",
    "build:esm": "node esbuild.mjs",
    "build:zig": "zig build",
    "wrangler:publish": "wrangler publish",
    "test": "zig build && node esbuild.mjs && NODE_OPTIONS=--experimental-vm-modules ava --verbose ./test/**/*.spec.ts"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@cloudflare/workers-types": "^3.14.1",
    "@types/node": "^18.7.10",
    "ava": "^4.3.1",
    "esbuild": "^0.14.53",
    "hono": "^2.0.8",
    "itty-router": "^2.6.1",
    "miniflare": "^2.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wrangler": "2.0.26"
  }
}